				<showOnRoof>true</showOnRoof>
				<heatPerPulse>15</heatPerPulse>
				<energyToFire>50</energyToFire>
				<optRange>100</optRange>
				<maxRange>100</maxRange>
				<projectileSpeed>3</projectileSpeed>
				<threat>10</threat>
				<groundDefense>true</groundDefense>
