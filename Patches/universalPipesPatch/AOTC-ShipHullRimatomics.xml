<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Rimatomics</li>
		</mods>
			<match Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="shipStarSteel_Beamtech" or defName="shipStarSteelShipAirlockArchotech" or defName="shipeternium_Beamtech"]/comps</xpath>
					<value>
						<li Class="Rimatomics.CompProperties_Pipe">
							<mode>HighVoltage</mode>
						</li>
						<li Class="Rimatomics.CompProperties_Pipe">
							<mode>Loom</mode>
						</li>
						<li Class="Rimatomics.CompProperties_Pipe">
							<mode>Steam</mode>
						</li>
						<li Class="Rimatomics.CompProperties_Pipe">
							<mode>ColdWater</mode>
						</li>
						<li Class="Rimatomics.CompProperties_Pipe">
							<mode>Cooling</mode>
						</li>
					</value>
			</match>
	</Operation>
</Patch>